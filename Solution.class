class Solution {
    public int maxNonOverlapping(int[] nums, int target) {
        Set<Integer> prefixes = new HashSet();
        int answer = 0, sum = 0;
        for(int i = 0; i < nums.length; i++) {
            sum += nums[i];
            if(prefixes.contains(sum - target) || sum == target) {
                prefixes = new HashSet();
                sum = 0;
                answer++;
            } else {
                prefixes.add(sum);
            }
        }
        return answer;
    }
}
